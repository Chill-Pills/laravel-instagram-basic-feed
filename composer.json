{
    "name": "chillpills/laravel-instagram-basic-feed",
    "description": "Laravel package that connect to Instagram's new API Basic Display , retrieve personal user's posts and keep them in cache, up to date with Scheduler and take care to refresh the token's every two months.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "dib258",
            "email": "ludo@chillpills.io"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0|^7.0",
        "espresso-dev/instagram-basic-display-php": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.1"
    },
    "autoload" : {
        "psr-4" : {
            "ChillPills\\InstagramBasicFeed\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "ChillPills\\InstagramBasicFeed\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChillPills\\InstagramBasicFeed\\InstagramBasicFeedServiceProvider"
            ],
            "aliases": {
                "InstagramBasicFeed": "ChillPills\\InstagramBasicFeed\\Facades\\InstagramBasicFeed"
            }
        }
    }
}
